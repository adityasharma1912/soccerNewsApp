<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aditya.girnar.rssparsing">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.aditya.girnar.rssparsing.application.SoccerApp"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.aditya.girnar.rssparsing.activity.FirstActivity"
            android:screenOrientation="portrait"
            android:icon="@drawable/icon"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.aditya.girnar.rssparsing.activity.SecondActivity" />
    </application>

</manifest>
